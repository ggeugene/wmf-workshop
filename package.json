{
  "name": "yalantis-workshop-wmf-app",
  "description": "Webpack Module Federation for Yalantis Workshop Nov 23th 2021",
  "version": "1.0.0",
  "author": "Yevhen Horodnichenko",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "wipe": "rm -fr node_modules apps/**/node_modules",
